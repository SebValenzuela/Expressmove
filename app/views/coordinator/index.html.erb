<p id="notice"><%= notice %></p>

<h1>Coordinator</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Direccion</th>
      <th>Email</th>
      <th>Telefono</th>
      <th>Control</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordinators.each do |coordinator| %>
      <tr>
        <td><%= coordinator.nombre %></td>
        <td><%= coordinator.rut %></td>
        <td><%= coordinator.direccion %></td>
        <td><%= coordinator.email %></td>
        <td><%= coordinator.telefono %></td>
        <td><%= coordinator.control %></td>
        <td><%= coordinator.fecha %></td>
        <td><%= link_to 'Show', coordinator %></td>
        <td><%= link_to 'Edit', edit_coordinator_path(coordinator) %></td>
        <td><%= link_to 'Destroy', coordinator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coordinator', new_coordinator_path %>
